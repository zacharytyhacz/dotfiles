! fonts =========================================================================================

URxvt.font: xft:Hermit:style=Light:size=10
URxvt.boldFont: xft:Hermit:style=Regular:size=10
URxvt.italicFont: xft:Hermit:style=Regular:size=9,9x15
URxvt.boltItalicFont: xft:Hermit:style=Regular:size=9,9x15
URxvt.lineSpace: 0
URxvt.letterSpace: -1

! URxvt.font:     xft:bitstream vera sans mono:size=9:antialias=true
! URxvt.boldFont: xft:bitstream vera sans mono:bold:size=9:antialias=true

! URxvt.font:     xft:fixedsys excelsior:regular:size=12,10x10
! URxvt.boldFont: xft:fixedsys excelsior:regular:size=12,10x10

! URxvt.font: xft:AcPlus Amstrad PC:style=Regular:size=10
! URxvt.boldFont: xft:AcPlus Amstrad PC:style=Regular:size=10
! URxvt.lineSpace: 5
! URxvt.letterSpace: 1

!
! config =========================================================================================

Xft.dpi: 96
Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

URxvt.perl-ext-common: default,clipboard
URxvt.clipboard.autocopy: true
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste
URxvt.geometry: 92x24
URxvt.internalBorder: 8
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.cursorColor: green
URxvt.saveline: 2048
URxvt.saveLines: 65535
URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.urgentOnBell: false
URxvt.iso14755: false
URxvt.dynamicColors: on
URxvt.antialias: 1

! transparency shit
URxvt.depth: 4
URxvt.transparent: false
URxvt.shading: 32

! =================== binds 

! scroll up
URxvt.keysym.Shift-Up: command:\033]720;1\007
! scroll down
URxvt.keysym.Shift-Down: command:\033]721;1\007

! jump words
URxvt.keysym.Control-Right: \033[1;5C
URxvt.keysym.Control-Left: \033[1;5D



